<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobio.sample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.mobio.sample.MobioApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AnalyticsSDK">
        <service
            android:name="com.mobio.sample.MyFirebaseMessagingService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name="com.mobio.sample.TransferSuccessActivity"
            android:exported="true"></activity>
        <activity
            android:name="com.mobio.sample.ConfirmTransferActivity"
            android:exported="true" />
        <activity
            android:name="com.mobio.sample.SendMoneyInActivity"
            android:exported="true" />
        <activity
            android:name="com.mobio.sample.SendMoneyActivity"
            android:exported="true" />
        <activity
            android:theme="@style/CustomTheme"
            android:name="com.mobio.sample.HomeActivity"
            android:exported="true" />
        <activity
            android:name="com.mobio.sample.SavingActivity"
            android:exported="true"
            android:launchMode="singleTop" />
        <activity
            android:name="com.mobio.sample.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:theme="@style/CustomTheme"
            android:name="com.mobio.sample.LoginActivity"
            android:exported="true" />

        <receiver
            android:name="com.mobio.sample.NotificationDismissedReceiver"
            android:exported="false" />
    </application>

</manifest>